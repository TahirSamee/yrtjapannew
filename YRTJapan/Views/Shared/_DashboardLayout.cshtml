<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <!--[if IE]>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <![endif]-->
    <meta name="description" content="">
    <meta name="author" content="ScriptsBundle">
    <title>YRT Japan</title>
    <!-- =-=-=-=-=-=-= Favicons Icon =-=-=-=-=-=-= -->
    <link rel="icon" href="~/images/favicon.ico" type="image/x-icon" />
    <!-- =-=-=-=-=-=-= Mobile Specific =-=-=-=-=-=-= -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <!-- =-=-=-=-=-=-= Bootstrap CSS Style =-=-=-=-=-=-= -->
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
    <!-- =-=-=-=-=-=-= Template CSS Style =-=-=-=-=-=-= -->
    <link rel="stylesheet" href="~/css/style.css">
    <!-- =-=-=-=-=-=-= Font Awesome =-=-=-=-=-=-= -->
    <link rel="stylesheet" href="~/font-awesome/flag-icons-master/flag-icons.min.css" type="text/css">
    <!-- =-=-=-=-=-=-= Flags Icons =-=-=-=-=-=-= -->
    <link rel="stylesheet" href="~/font-awesome/css/font-awesome.css" type="text/css">
    <!-- =-=-=-=-=-=-= Flat Icon =-=-=-=-=-=-= -->
    <link href="~/css/flaticon.css" rel="stylesheet">
    <!-- =-=-=-=-=-=-= Et Line Fonts =-=-=-=-=-=-= -->
    <link rel="stylesheet" href="~/css/et-line-fonts.css" type="text/css">
    <!-- =-=-=-=-=-=-= Menu Drop Down =-=-=-=-=-=-= -->
    <link rel="stylesheet" href="~/css/carspot-menu.css" type="text/css">
    <!-- =-=-=-=-=-=-= Animation =-=-=-=-=-=-= -->
    <link rel="stylesheet" href="~/css/animate.min.css" type="text/css">
    <!-- =-=-=-=-=-=-= Select Options =-=-=-=-=-=-= -->
    <link href="~/css/select2.min.css" rel="stylesheet" />
    <!-- =-=-=-=-=-=-= noUiSlider =-=-=-=-=-=-= -->
    <link href="~/css/nouislider.min.css" rel="stylesheet">
    <!-- =-=-=-=-=-=-= Listing Slider =-=-=-=-=-=-= -->
    <link href="~/css/slider.css" rel="stylesheet">
    <!-- =-=-=-=-=-=-= Owl carousel =-=-=-=-=-=-= -->
    <link rel="stylesheet" type="text/css" href="~/css/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="~/css/owl.theme.css">
    <!-- =-=-=-=-=-=-= Check boxes =-=-=-=-=-=-= -->
    <link href="~/skins/minimal/minimal.css" rel="stylesheet">
    <!-- =-=-=-=-=-=-= PrettyPhoto =-=-=-=-=-=-= -->
    <link rel="stylesheet" href="~/css/jquery.fancybox.min.css" type="text/css" />
    <!-- =-=-=-=-=-=-= Responsive Media =-=-=-=-=-=-= -->
    <link href="~/css/responsive-media.css" rel="stylesheet">
    <link href="~/css/responsive.css" rel="stylesheet">
    <!-- =-=-=-=-=-=-= Template Color =-=-=-=-=-=-= -->
    <link rel="stylesheet" id="color" href="~/css/colors/defualt.css">
    <link href="https://fonts.googleapis.com/css?family=Poppins:400,500,600%7CSource+Sans+Pro:400,400i,600" rel="stylesheet">
    <!-- JavaScripts -->
    <script src="~/js/modernizr.js"></script>
    <!-- Chart.js -->
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <!-- Bootstrap Icons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-icons/1.5.0/font/bootstrap-icons.min.css">
    <!-- DataTables CSS -->
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.25/css/jquery.dataTables.min.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <style>
        .sidebar {
            width: 15%;
            position: fixed;
            top: 0;
            left: 0;
            height: 100%;
            background: #000;
            padding-top: 0;
            border-right: 1px solid #dee2e6;
            z-index: 1000; /* Ensure sidebar is always on top */
        }

            .sidebar h2 {
                color: #fff;
                text-align: center;
                background-color: #d60000;
                padding: 10px 0;
                margin: 0;
            }

            .sidebar ul {
                list-style-type: none;
                padding: 0;
            }

                .sidebar ul li {
                    padding: 10px;
                    text-align: center;
                }

                    .sidebar ul li i {
                        display: block;
                        margin: 0 auto 10px;
                        font-size: 20px;
                        color: #fff;
                    }

                    .sidebar ul li a {
                        color: #fff;
                        text-decoration: none;
                        font-size: 14px;
                        display: block;
                    }

                        .sidebar ul li a:hover,
                        .sidebar ul li a:focus,
                        .sidebar ul li a.active {
                            color: #ff0000;
                        }

        .content {
            margin-left: 15%;
            padding: 20px;
        }

        .content-section {
            display: none;
        }

            .content-section.active {
                display: block;
            }

        .chart-container {
            width: 45%;
            margin: 10px;
            display: inline-block;
        }

        .table-container {
            margin-top: 20px;
        }

        .table-striped tbody tr:nth-of-type(odd) {
            background-color: rgba(0, 0, 0, 0.05);
        }

        .table-dark th {
            background-color: #000 !important;
            color: #fff;
        }

        .table td,
        .table th {
            font-size: 12px;
        }

        .dataTables_wrapper .dataTables_paginate .paginate_button {
            padding: 0;
            margin-left: 0;
            font-size: 12px;
        }

        .dataTables_wrapper .dataTables_length select,
        .dataTables_wrapper .dataTables_filter input {
            font-size: 12px;
        }
    </style>
</head>

<body>
    <!-- =-=-=-=-=-=-= Sidebar =-=-=-=-=-=-= -->
    <div class="sidebar">
        <h2><i class="fi fi-jp"></i> <span class="">Yrt Japan</span></h2>
        <ul class="makerCompanisList">
            <li>
                <a href="/Admin/Dashboard/Index">
                    <i class="bi bi-speedometer2"></i>
                    <span>Dashboard</span>
                </a>
            </li>
            <li>
                <a href="/Admin/Stock/Index">
                    <i class="bi bi-speedometer2"></i>
                    <span>Stock</span>
                </a>
            </li>
            <li>
                <a href="/Admin/User/Index">
                    <i class="bi bi-people"></i>
                    <span>Users</span>
                </a>
            </li>
            <li>
                <a href="/Admin/inquiry/Index">
                    <i class="bi bi-question-circle"></i>
                    <span>Inquiries</span>
                </a>
            </li>
            <li>
                <a href="/Admin/Favourite/Index" >
                    <i class="bi bi-heart"></i>
                    <span>Favourites</span>
                </a>
            </li>
        </ul>
    </div>

    <!-- =-=-=-=-=-=-= Main Content Area =-=-=-=-=-=-= -->
    <div class="content">
        <header class="header-top" style="background:#000;">
            <div class="container">
                <div class="row justify-content-around">
                    <!-- Header Top Left -->
                    <div class="header-top-left col-md-7 col-sm-6 col-xs-12 hidden-xs">
                        <ul class="listnone">
                            <li class="text-white">YRT Japan The World's Largest Used Car Exporter</li>
                            <li class="text-white">|</li>
                            <li class="text-white">Total Cars In Stock: 12,34567</li>
                            <li class="text-white"><a href="about.html"><i class="fa fa-clock-o"></i> Japan time: 08:47 PM</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </header>
    </div>
    <!-- =-=-=-=-=-=-= Main Content Area =-=-=-=-=-=-= -->
    <div class="main-content-area" style="margin-left:15%">
        @RenderBody()
    </div>

    <!-- =-=-=-=-=-=-= JQUERY =-=-=-=-=-=-= -->
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <!-- Bootstrap Core Css  -->
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <!-- Jquery Easing -->
    <script src="~/js/easing.js"></script>
    <!-- Menu Hover  -->
    <script src="~/js/carspot-menu.js"></script>
    <!-- Jquery Appear Plugin -->
    <script src="~/js/jquery.appear.min.js"></script>
    <!-- Numbers Animation   -->
    <script src="~/js/jquery.countTo.js"></script>
    <!-- Jquery Select Options  -->
    <script src="~/js/select2.min.js"></script>
    <!-- noUiSlider -->
    <script src="~/js/nouislider.all.min.js"></script>
    <!-- Carousel Slider  -->
    <script src="~/js/carousel.min.js"></script>
    <script src="~/js/slide.js"></script>
    <!-- Image Loaded  -->
    <script src="~/js/imagesloaded.js"></script>
    <script src="~/js/isotope.min.js"></script>
    <!-- CheckBoxes  -->
    <script src="~/js/icheck.min.js"></script>
    <!-- Jquery Migration  -->
    <script src="~/js/jquery-migrate.min.js"></script>
    <!-- Style Switcher -->
    <script src="~/js/color-switcher.js"></script>
    <!-- PrettyPhoto -->
    <script src="~/js/jquery.fancybox.min.js"></script>
    <!-- Wow Animation -->
    <script src="~/js/wow.js"></script>
    <!-- DataTables JS -->
    <script src="https://cdn.datatables.net/1.10.25/js/jquery.dataTables.min.js"></script>
    <!-- Template Core JS -->
    <script src="~/js/custom.js"></script>
    <!-- Render Additional Page-Specific Scripts -->
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
