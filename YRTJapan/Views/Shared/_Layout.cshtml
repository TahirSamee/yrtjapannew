    <script type="text/javascript">
        function googleTranslateElementInit() {
            new google.translate.TranslateElement({
                pageLanguage: 'en',
                autoDisplay: false
            }, 'google_translate_element');
        }

        function translateLanguage(lang) {
            const flagIcon = document.querySelector('#languageToggleIcon');
            if (flagIcon) {
                flagIcon.className = (lang === 'ja') ? 'fi fi-jp' : 'fi fi-us';
            }
            debugger
            let attempts = 0;
            const interval = setInterval(() => {
                const combo = document.querySelector('select.goog-te-combo');
                if (combo) {
                    clearInterval(interval);

                    if (combo.value === lang) {
                        // Force translation by temporarily switching to dummy (Spanish) first
                        combo.value = 'es';
                        combo.dispatchEvent(new Event('change'));

                        setTimeout(() => {
                            combo.value = lang;
                            combo.dispatchEvent(new Event('change'));
                            console.log("Language changed to:", lang);
                        }, 200); // Delay long enough for Google to recognize a real change
                    } else {
                        combo.value = lang;
                        combo.dispatchEvent(new Event('change'));
                        console.log("Language changed to:", lang);
                    }

                } else if (++attempts > 20) {
                    clearInterval(interval);
                    console.warn("Google Translate not ready");
                }
            }, 100);
        }
    </script>

